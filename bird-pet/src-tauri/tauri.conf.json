{
  "productName": "bird-pet",
  "version": "0.5.0",
  "identifier": "com.administrator.bird-pet",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "macOSPrivateApi": true,
    "windows": [
      {
    "label": "main",
    "title": "BirdPet",
    "width": 150,
    "height": 150,
    "resizable": false,
    "decorations": false,
    "transparent": true,
    "alwaysOnTop": true,
    "shadow": false,
    "skipTaskbar": true
  }
    ],
    "security": {
      "csp": null,
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "msi",
    "createUpdaterArtifacts": "v1Compatible",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/yin1895/memo/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDJDNTg2NTY4NkFFRkFFNEMKUldSTXJ1OXFhR1ZZTFBLSGp1UUZBR1lMZ0JscUhibDBIeW16WG9PZldQdXBYUWNKL0pvZEJMOUcK",
      "windows": {
        "installMode": "passive"
      }
    }
  }
}
